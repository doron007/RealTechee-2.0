    "test:e2e:headed": "playwright test --headed",
    "test:validate-env": "node scripts/validate-test-environment.js",
    "test:e2e:forms": "playwright test form-validation",
    "test:e2e:forms:headed": "playwright test --headed form-validation",
    "test:e2e:notifications": "playwright test admin-notifications-screenshot.spec.js",
  "test:e2e:notifications:headed": "playwright test --headed admin-notifications-screenshot.spec.js",
  "verify:env-contract": "node scripts/verify-env-contract.js",
  "invoke:status-processor": "node scripts/invoke-status-processor-local.js",
